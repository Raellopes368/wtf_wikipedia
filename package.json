{
  "name": "wtf_wikipedia",
  "description": "parse wikiscript into json",
  "version": "7.3.0",
  "author": "Spencer Kelly <spencermountain@gmail.com> (http://spencermounta.in)",
  "repository": {
    "type": "git",
    "url": "git://github.com/spencermountain/wtf_wikipedia.git"
  },
  "main": "src/index.js",
  "unpkg": "builds/wtf_wikipedia.min.js",
  "scripts": {
    "start": "node ./scripts/demo.js",
    "test": "node ./scripts/test.js",
    "test-spec": "tape ./tests/*.test.js | tap-spec",
    "coverage": "node scripts/coverage.js",
    "postpublish": "node ./scripts/coverage.js",
    "testb": "TESTENV=prod node ./scripts/test.js",
    "watch": "amble ./scratch.js",
    "build": "node ./scripts/build.js",
    "lint": "eslint ./src/**/*.js"
  },
  "bin": {
    "wtf_wikipedia": "./bin/wtf.js"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "printWidth": 100
  },
  "files": [
    "builds",
    "api",
    "src",
    "bin"
  ],
  "keywords": [
    "wikipedia",
    "wikimedia",
    "wikipedia markup",
    "wikiscript"
  ],
  "dependencies": {
    "cross-fetch": "3.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "amble": "0.0.7",
    "babelify": "10.0.0",
    "browserify": "16.2.3",
    "codecov": "3.3.0",
    "derequire": "2.0.6",
    "nyc": "14.0.0",
    "shelljs": "0.8.3",
    "tap-dancer": "0.2.0",
    "tap-spec": "5.0.0",
    "tape": "4.10.1",
    "terser": "3.17.0"
  },
  "license": "MIT"
}
